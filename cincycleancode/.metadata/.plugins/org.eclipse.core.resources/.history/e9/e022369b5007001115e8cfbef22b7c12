package org.codingdojo.roman.numeral.type;

public class Digit {
	
	private int value;

	@Override
	public int hashCode() {
		final int prime = 31;
		int result = 1;
		result = prime * result + value;
		return result;
	}

	@Override
	public boolean equals(Object obj) {
		if (this == obj)
			return true;
		if (obj == null)
			return false;
		if (getClass() != obj.getClass())
			return false;
		Digit other = (Digit) obj;
		if (value != other.value)
			return false;
		return true;
	}

}
